{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Description": {
      "type": "text"
    },
    "Title": {
      "type": "text"
    },
    "Price": {
      "type": "float"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Nike",
        "Adidas",
        "Puma",
        "Bacca Bucci",
        "US POLO"
      ]
    },
    "Quantity": {
      "type": "integer",
      "required": true,
      "max": 20,
      "min": 1
    },
    "Product_Images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
